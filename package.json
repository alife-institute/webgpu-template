{
  "name": "webgpu-template",
  "version": "1.0.0",
  "description": "A template project for WebGPU development using TypeScript and Webpack.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --port 5500",
    "watch": "webpack --watch",
    "build": "webpack",
    "capture": "node tools/capture-simulation.js",
    "capture:quick": "node tools/capture-simulation.js --screenshots 5 --interval 1000",
    "capture:detailed": "node tools/capture-simulation.js --screenshots 30 --interval 200"
  },
  "license": "MIT",
  "dependencies": {
    "@webgpu/types": "^0.1.46",
    "html-webpack-plugin": "^5.6.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "devDependencies": {
    "puppeteer": "^24.29.1"
  }
}
